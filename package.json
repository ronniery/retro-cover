{
  "name": "retro-cover",
  "version": "1.0.0",
  "description": "A node.js library to search, parse and fetch covers from https://www.thecoverproject.net/",
  "scripts": {
    "test": "jest --coverage --runInBand",
    "build": "tsc -p tsconfig.build.json",
    "lint": "eslint ./src --ext .js,.ts",
    "lint:fix": "npm run lint --fix",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\"",
    "prepare": "ts-patch install -s"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ronniery/retro-cover.git"
  },
  "keywords": [
    "retro",
    "game",
    "cover",
    "thecoverproject"
  ],
  "author": "Ronniery Borges Correa",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ronniery/retro-cover/issues"
  },
  "homepage": "https://github.com/ronniery/retro-cover#readme",
  "devDependencies": {
    "@types/cheerio": "^0.22.35",
    "@types/jest": "^29.5.11",
    "@types/lodash.chunk": "^4.2.9",
    "@types/lodash.isdate": "^4.0.9",
    "@types/lodash.isempty": "^4.4.9",
    "@types/lodash.omit": "^4.5.9",
    "@types/lodash.orderby": "^4.6.9",
    "@types/node": "^20.10.5",
    "@typescript-eslint/eslint-plugin": "^6.15.0",
    "@typescript-eslint/parser": "^6.15.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "jest": "^29.7.0",
    "jest-extended": "^4.0.2",
    "nock": "^13.4.0",
    "prettier": "3.1.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "ts-patch": "^3.1.1",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "axios": "^1.6.2",
    "axios-cache-interceptor": "^1.4.0",
    "axios-request-throttle": "^1.0.0",
    "axios-retry": "^4.0.0",
    "cheerio": "^1.0.0-rc.12",
    "html-entities": "^2.4.0",
    "lodash.chunk": "^4.2.0",
    "lodash.isdate": "^4.0.1",
    "lodash.isempty": "^4.4.0",
    "lodash.omit": "^4.5.0",
    "lodash.orderby": "^4.6.0",
    "robots-parser": "^3.0.1"
  }
}
